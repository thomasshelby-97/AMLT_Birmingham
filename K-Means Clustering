    "from sklearn.metrics import silhouette_score\n",
    "\n",
    "SS = []\n",
    "for k in range (2, 25):\n",
    "    kmeans = KMeans(n_clusters = k, random_state = 10)\n",
    "    kmeans.fit(X)\n",
    "    SS.append(silhouette_score(X, kmeans.predict(X)))\n",
    "plt.plot(range (2, 25), SS);"
